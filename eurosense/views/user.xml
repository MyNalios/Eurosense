<odoo>

    <record id="eurosense_users_form" model="ir.ui.view">
        <field name="name">eurosense.users.form.inherit</field>
        <field name="model">res.users</field>
        <field eval="8" name="priority"/>
        <field name="inherit_id" ref="base.view_users_form"/>
        <field name="arch" type="xml">
            <page name="references" position="after">
                <page string="Allowed contacts invoices">
                    <field name="allowed_contact_ids" widget="many2many_tags" options="{'no_create': True}"/>
                </page>
            </page>
        </field>
    </record>
 
</odoo>